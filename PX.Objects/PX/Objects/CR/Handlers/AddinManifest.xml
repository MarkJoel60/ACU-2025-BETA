<?xml version="1.0" encoding="UTF-8"?>
<!--Created:cb85b80c-f585-40ff-8bfc-12ff4d0e34a9-->
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="MailApp"
		xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0"
	xmlns:mailappor="http://schemas.microsoft.com/office/mailappversionoverrides/1.0">
  <Id>80093e9f-b8cf-4ca8-b81f-d87833fb56b4</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Acumatica</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Acumatica" />
  <Description DefaultValue="Acumatica"/>
	<IconUrl DefaultValue="{domain}Frames/Outlook/Acumatica_Teardrop_RGB_MayanBlue_64X64.png" />
	<HighResolutionIconUrl DefaultValue="{domain}Frames/Outlook/Acumatica_Teardrop_RGB_MayanBlue_128X128.png" />
	<SupportUrl DefaultValue="https://www.acumatica.com " />
  <AppDomains>
    <AppDomain>{domain}</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Mailbox" />
  </Hosts>
  <Requirements>
    <Sets>
      <Set Name="MailBox" MinVersion="1.1" />
    </Sets>
  </Requirements>
  <FormSettings>
    <Form xsi:type="ItemRead">
      <DesktopSettings>
		  <!--SourceLocation DefaultValue="{domain}OutlookAddIn/Token?url=..%2FPages%2FCR%2FOU201000.aspx"/-->
		  <SourceLocation DefaultValue="{domain}Frames/Outlook/FirstRun.html"/>
		  <RequestedHeight>450</RequestedHeight>
      </DesktopSettings>
    </Form>
    <Form xsi:type="ItemEdit">
      <DesktopSettings>
        <!--<SourceLocation DefaultValue="{domain}Pages/CR/ou202000.aspx"/>-->
        <SourceLocation DefaultValue="{domain}OutlookAddIn/Token?url=..%2FPages%2FCR%2FOU202000.aspx"/>
      </DesktopSettings>
    </Form>
  </FormSettings>
  <Permissions>ReadWriteItem</Permissions>
  <Rule xsi:type="RuleCollection" Mode="Or">
    <Rule xsi:type="ItemIs" ItemType="Message" FormType="Edit" />
    <Rule xsi:type="ItemIs" ItemType="Message" FormType="Read" />
    <Rule xsi:type="ItemHasAttachment" />
  </Rule>
  <DisableEntityHighlighting>false</DisableEntityHighlighting>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Description resid="residDescription" />
    <Requirements>
      <bt:Sets DefaultMinVersion="1.3">
        <bt:Set Name="Mailbox" />
      </bt:Sets>
    </Requirements>
    <Hosts>
      <Host xsi:type="MailHost">
        <DesktopFormFactor>
          <ExtensionPoint xsi:type="MessageReadCommandSurface">
            <OfficeTab id="TabDefault">
              <Group id="msgreadTabMessage.grp1">
                <Label resid="groupLabel" />
                <Control xsi:type="Button" id="msgreadTabMessage.grp1.btnView">
                  <Label resid="buttonLabelRead" />
                  <!-- If you want tool tips -->
                  <Supertip>
                    <Title resid="superTitleRead" />
                    <Description resid="superTipRead" />
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="icon1_16x16" />
                    <bt:Image size="32" resid="icon1_32x32" />
                    <bt:Image size="64" resid="icon1_64x64" />
                    <bt:Image size="80" resid="icon1_80x80" />
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="taskPaneReadUrl" />
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <!-- Upload the icons to your own servers and replace the DefaultValue URLs to the images-->
		  <bt:Image id="icon1_16x16" DefaultValue="{domain}Frames/Outlook/Acumatica_Teardrop_RGB_MayanBlue_16X16.png" ></bt:Image>
		  <bt:Image id="icon1_32x32" DefaultValue="{domain}Frames/Outlook/Acumatica_Teardrop_RGB_MayanBlue_32X32.png" ></bt:Image>
		  <bt:Image id="icon1_64x64" DefaultValue="{domain}Frames/Outlook/Acumatica_Teardrop_RGB_MayanBlue_64X64.png" ></bt:Image>
		  <bt:Image id="icon1_80x80" DefaultValue="{domain}Frames/Outlook/Acumatica_Teardrop_RGB_MayanBlue_80X80.png"></bt:Image>
      </bt:Images>
      <bt:Urls>
		  <!--bt:Url id="taskPaneComposeUrl" DefaultValue="{domain}outlook/Pages/CR/ou202000.aspx"></bt:Url-->
		  <!--bt:Url id="taskPaneComposeUrl" DefaultValue="{domain}OutlookAddIn/Token?url=..%2FPages%2FCR%2FOU202000.aspx"></bt:Url-->
		  <!--bt:Url id="taskPaneReadUrl" DefaultValue="{domain}outlook/Pages/CR/ou201000.aspx"></bt:Url-->
		  <!--bt:Url id="taskPaneReadUrl" DefaultValue="{domain}OutlookAddIn/Token?url=..%2FPages%2FCR%2FOU201000.aspx"></bt:Url-->
		  <bt:Url id="taskPaneReadUrl" DefaultValue="{domain}Frames/Outlook/FirstRun.html"></bt:Url>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="groupLabel" DefaultValue="Acumatica"></bt:String>
		  <!--bt:String id="buttonLabelCompose" DefaultValue="Acumatica ERP"></bt:String-->
        <bt:String id="buttonLabelRead" DefaultValue="Acumatica"></bt:String>
        <!-- If you want tool tips for the buttons this is where the title of the tips reside -->
		  <!--bt:String id="superTitleCompose" DefaultValue="This is the bold title for the compose tip"></bt:String-->
        <bt:String id="superTitleRead" DefaultValue="Acumatica Add-In for Outlook"></bt:String>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="residDescription" DefaultValue="Acumatica"></bt:String>
        <!-- If you want tool tips for the buttons this is where the strings reside -->
        <!--bt:String id="superTipCompose" DefaultValue="This is the compose tip that's below the bolded tip"></bt:String-->
        <bt:String id="superTipRead" DefaultValue="The Acumatica add-in for Outlook helps you handle leads, contacts, and employees. It also allows you to create new opportunities and cases right from your mailbox, as well as log your communication activities."></bt:String>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>